{
	"type": "config",
	"id": "conf.service.webdesk",
	"title": "WebDesk",
	"queryinfo": {
		"xpath": "//services/webdesk",
		"iterable": false
	},
	"properties": {
		"enable": {
			"type": "boolean",
			"default": 0
		},
		"name": {
			"type": "string",
			"default": "WebDesk for OpenMediaVault"
		},
		"port": {
			"type": "integer",
			"minimum": 1,
			"maximum": 65535,
			"default": 8200
		},
		"shares": {
			"type": "object",
			"properties": {
				"share": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"uuid": {
								"type": "string",
								"format": "uuidv4"
							},
							"sharedfolderref": {
								"type": "string",
								"format": "uuidv4"
							},
							"mtype": {
								"type": "string"
							}
						}
					}
				}
			}
		}
	}
}
